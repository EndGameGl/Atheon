{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "AllowedHosts": "*",
  "Database": {
    "CurrentMode": "Sqlite",
    "Databases": {
      "Sqlite": {
        "ConnectionString": "Data Source=db.sqlite; Cache=Shared; Pooling=True"
      }
    },
    "Tables": {
      "SettingsStorage": {
        "Columns": [
          {
            "Name": "Key",
            "Type": {
              "Sqlite": "TEXT"
            },
            "PrimaryKey": true,
            "NotNull": true
          },
          {
            "Name": "Value",
            "Type": {
              "Sqlite": "TEXT"
            }
          }
        ]
      }
    }
  }
}